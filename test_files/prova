 int n := 0;
 int i := 5;
 int j:= 3;
int a, b,c :=0;

proc f()int,int:

    b:=n;
    c:=a;

    -> b,c
corp;

 proc main() void:
 int q,e;
 string t:="dsag";
 if t="dsag" then
    write("Factorial of ", n, " is ", f(), "\n");
    fi;
        n:=i+j;
        a:=n*j;

        q,e:= f();
        write("Factorial of ", n, " is ", f(), "\n");
    ->
 corp;